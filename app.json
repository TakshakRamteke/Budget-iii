{
    "expo": {
        "name": "Budgetiii",
        "slug": "budgetiii",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "budgetiii",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#161616"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.takshakramteke.budgetiii",
            "intentFilters": [
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": [
                        {
                            "scheme": "https",
                            "host": "*.budgetiii.io",
                            "pathPrefix": "/records"
                        }
                    ],
                    "category": ["BROWSABLE", "DEFAULT"]
                }
            ]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-router"],
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "66c4fa93-66d1-4603-bc40-f5026b74d8b5"
            }
        },
        "owner": "takshakramteke"
    }
}
